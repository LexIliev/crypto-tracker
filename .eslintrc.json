{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "rules": {
    "no-use-before-define": [0],
    "react/jsx-filename-extension": [0],
    "react/prop-types": [0],
    "react/destructuring-assignment": [0],
    "react/require-default-props": [0],
    "react/jsx-one-expression-per-line": [0],
    "import/no-extraneous-dependencies": [0],
    "comma-dangle": [0],
    "no-dupe-keys": [0],
    "func-names": [0],
    "camelcase": [0]
  },
  "globals": {
    "fetch": false
  }
}
